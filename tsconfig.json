{
    "compilerOptions": {
        "target": "es2021",
        "module": "CommonJS",
        "lib": [
            "es2021"
        ],
        "declaration": true,
        "sourceMap": true,
        "outDir": "./dist",
        "rootDir": "./src",
        "strict": false,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "skipLibCheck": true,
        "importHelpers": true,
        "forceConsistentCasingInFileNames": true,
    },
}